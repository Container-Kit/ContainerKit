{
    "name": "container-kit",
    "version": "0.3.0",
    "description": "",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "pnpm db:generate && vite build",
        "preview": "vite preview",
        "tauri": "tauri",
        "tauri:build": "source .env && TAURI_SIGNING_PRIVATE_KEY_PASSWORD=$TAURI_SIGNING_PRIVATE_KEY_PASSWORD TAURI_SIGNING_PRIVATE_KEY=$TAURI_SIGNING_PRIVATE_KEY tauri build --target aarch64-apple-darwin",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check .",
        "db:generate": "drizzle-kit generate && pnpm db:migrations:rust",
        "db:migrations:rust": "tsx scripts/generate-migrations.ts"
    },
    "license": "MIT",
    "dependencies": {
