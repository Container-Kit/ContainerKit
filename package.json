        "@tauri-apps/plugin-sql": "~2.3.0",
        "@tauri-apps/plugin-store": "~2.3.0",
        "@tauri-apps/plugin-updater": "~2.9.0",
        "@tauri-apps/plugin-window-state": "~2.3.0",
        "@xterm/xterm": "^5.5.0",
